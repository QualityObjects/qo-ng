{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "qo-ng-common-ui": {
      "projectType": "library",
      "root": "projects/qo-ng-common-ui",
      "sourceRoot": "projects/qo-ng-common-ui/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/qo-ng-common-ui/tsconfig.lib.json",
            "project": "projects/qo-ng-common-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/qo-ng-common-ui/tsconfig.lib.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/qo-ng-common-ui/src/test.ts",
            "tsConfig": "projects/qo-ng-common-ui/tsconfig.spec.json",
            "karmaConfig": "projects/qo-ng-common-ui/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/qo-ng-common-ui/tsconfig.lib.json",
              "projects/qo-ng-common-ui/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "the-table": {
      "projectType": "library",
      "root": "projects/the-table",
      "sourceRoot": "projects/the-table/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/the-table/tsconfig.lib.json",
            "project": "projects/the-table/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/the-table/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/the-table/src/test.ts",
            "tsConfig": "projects/the-table/tsconfig.spec.json",
            "karmaConfig": "projects/the-table/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/the-table/tsconfig.lib.json",
              "projects/the-table/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "qo-ng-common-ui",
  "cli": {
    "analytics": "ca1f10d8-67cc-4ac3-9dac-360963863945"
  }
}